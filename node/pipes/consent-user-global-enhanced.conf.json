{
  "_id": "consent-user-global-enhanced",
  "type": "pipe",
  "source": {
    "type": "dataset",
    "dataset": "consent-user-global"
  },
  "transform": {
    "type": "dtl",
    "rules": {
      "default": [
        ["copy", "*"],
        ["add", "userinfo",
          ["hops", {
            "datasets": ["global-person gp"],
            "where": ["eq", "_S.EmailAddress", "gp.EmailAddress"]
          }]
        ]
      ]
    }
  },
  "metadata": {
    "tags": ["consent"]
  }
}
