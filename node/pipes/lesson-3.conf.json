{
  "_id": "lesson-3",
  "type": "pipe",
  "source": {
    "type": "merge",
    "datasets": ["lesson-2 ls2", "firebase-person fbp", "azure-person azp"],
    "equality": [
      ["eq", "ls2.personalno", "azp.Birthday"],
      ["eq", "azp.EmailAddress", "fbp.EmailAddress"]
    ],
    "identity": "first",
    "strategy": "compact",
    "version": 2
  },
  "transform": [{
    "type": "dtl",
    "rules": {
      "default": [
        ["copy", "*"]
      ]
    }
  }]
}
