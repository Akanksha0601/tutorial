{
  "_id": "azure-person",
  "type": "pipe",
  "source": {
    "type": "csv",
    "system": "sesam-datasource-azure",
    "blacklist": ["Password"],
    "delimiter": ",",
    "encoding": "utf-8",
    "primary_key": "Birthday",
    "url": "/file/sesam-training/data/test_people_sesam_training1.csv"
  },
  "transform": {
    "type": "dtl",
    "rules": {
      "default": [
        ["copy", "*"],
        ["remove", "City"]
      ]
    }
  },
  "pump": {
    "cron_expression": "0 * * * ?"
  }
}
